{
	"bail": 1,
	"verbose": true,
	"moduleNameMapper": {
		"@(utils|services|functions)(.*)$": "<rootDir>/$1/$2/"
	},
	"collectCoverageFrom": [
		"./**/*.js",
		"!babel.config.js",
		"!.eslintrc.js",
		"!jest.config.js",
		"!utils/*",
		"!webpack.config.js",
		"!coverage/**/*.js",
		"!.webpack/**/*.*"
	]
}
