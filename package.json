{
	"name": "serverless-template-todo",
	"version": "0.0.1",
	"description": "A serverless template",
	"main": "index.js",
	"scripts": {
		"-----------linting-----------": "",
		"format": "prettier-standard './**/**/*.js'",
		"lint": "npm run lint:js",
		"lint:eslint": "eslint --ignore-path .gitignore .",
		"lint:eslint:fix": "eslint --ignore-path .gitignore --fix",
		"lint:js": "npm run lint:eslint -- . ",
		"lint:staged": "lint-staged",
		"-----------test-----------": "",
		"test": "jest --coverage --silent",
		"test:local": "jest --coverage",
		"-----------build & deployment-----------": "",
		"build": "sls package --stage=dev",
		"deploy:dev": "sls deploy --stage=dev",
		"-----------dev-----------": "",
		"start:offline": "pnpm debug && sls offline start --stage=local | pino-pretty -c -t -C -f",
		"debug": "export SLS_DEBUG=*",
		"local:db:up": "docker compose up -d",
		"model:generate": "npx sequelize-cli model:generate",
		"db:migrate": "npx sequelize-cli db:migrate"
	},
	"engines": {
		"node": "16"
	},
	"author": "deepak-wednesday",
	"license": "ISC",
	"dependencies": {
		"@aws-sdk/client-sns": "^3.154.0",
		"@babel/plugin-transform-regenerator": "^7.14.5",
		"@babel/plugin-transform-runtime": "^7.15.0",
		"@babel/runtime": "^7.15.4",
		"@middy/core": "^2.5.3",
		"@middy/do-not-wait-for-empty-event-loop": "^2.5.3",
		"@middy/error-logger": "^3.3.0",
		"@middy/http-cors": "^2.5.4",
		"@middy/http-event-normalizer": "^3.3.0",
		"@middy/http-json-body-parser": "^2.5.3",
		"@middy/http-response-serializer": "^2.5.3",
		"@middy/util": "^2.5.7",
		"@middy/validator": "^2.5.3",
		"aws-sdk": "^2.1325.0",
		"axios": "^0.24.0",
		"http-errors": "^2.0.0",
		"joi": "^17.7.1",
		"lambda-log": "^3.1.0",
		"lodash": "^4.17.21",
		"middy-middleware-json-error-handler": "^3.0.0",
		"mysql2": "^3.1.2",
		"pg-hstore": "^2.3.4",
		"pino": "7",
		"pino-lambda": "^4.0.0",
		"sequelize": "^6.25.5",
		"serverless-offline": "^12.0.4",
		"source-map-support": "^0.5.19"
	},
	"devDependencies": {
		"@aws-sdk/client-ses": "^3.192.0",
		"@aws-sdk/client-sqs": "^3.163.0",
		"@aws-sdk/client-ssm": "^3.199.0",
		"@babel/core": "^7.20.12",
		"@babel/preset-env": "^7.18.10",
		"@babel/register": "^7.21.0",
		"aws-sdk-client-mock": "^1.0.0",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^27.2.1",
		"babel-loader": "^8.1.0",
		"babel-preset-env": "^1.7.0",
		"core-js": "3",
		"esbuild": "<0.17",
		"eslint": "^7.32.0 || ^8.2.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^4.1.0",
		"eslint-config-prettier-standard": "^3.0.1",
		"eslint-config-standard": "^14.1.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-node": "^0.3.7",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-node": "^10.0.0",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-standard": "^4.0.1",
		"jest": "^27.2.1",
		"jest-environment-jsdom": "^27.2.0",
		"lint-staged": "^9.4.2",
		"mockdate": "^3.0.2",
		"pino-pretty": "^9.4.0",
		"pre-commit": "1.2.2",
		"prettier": "1.17.0",
		"prettier-config-standard": "^1.0.1",
		"regenerator-runtime": "^0.13.9",
		"sequelize-cli": "^6.6.0",
		"serverless": "^3.28.1",
		"serverless-dotenv-plugin": "^3.0.0",
		"serverless-esbuild": "^1.39.0",
		"serverless-offline": "^8.1.0",
		"webpack": "^4.44.1",
		"webpack-node-externals": "^2.5.2"
	},
	"precommit": "lint:staged",
	"lint-staged": {
		"*.js": [
			"npm run lint:eslint:fix .",
			"git add --force",
			"jest --findRelatedTests $STAGED_FILES"
		],
		"*.json": [
			"prettier --write",
			"git add --force"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"repository": "git@github.com:wednesday-solutions/serverless-template-todo.git"
}
