{
	"compilerOptions": {
		"target": "es2016",
		"lib": [],
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
			"@functions/*": ["functions/*"],
			"@utils/*": ["utils/*"],
			"@services/*": ["services/*"]
		}
	}
}
